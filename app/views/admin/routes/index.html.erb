<h1><%= t('.title') %></h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t('.table_title') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @routes.each do |route| %>
      <%= render route %>
    <% end %>
  </tbody>
</table>

<p> <%= link_to t('.new'), new_admin_route_path, :class => "btn btn-primary" %>
<%= link_to t('common.back'), welcome_index_path, :class => "btn btn-default" %> </p>
